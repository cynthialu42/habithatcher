  "bugs": {
    "url": "https://github.com/cynthialu42/habithatcher/issues"
  },
  "homepage": "https://github.com/cynthialu42/habithatcher#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.2.4",
    "nodemon": "^1.18.3"
  }
}
